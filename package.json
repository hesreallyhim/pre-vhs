{
  "name": "pre-vhs",
  "version": "0.1.0",
  "description": "An intuitive, highly configurable pre-processor for writing charmbracelet/vhs scripts with more complex command sequences in fewer words",
  "main": "src/index.js",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "vitest": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hesreallyhim/pre-vhs.git"
  },
  "author": "Really Him",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hesreallyhim/pre-vhs/issues"
  },
  "homepage": "https://github.com/hesreallyhim/pre-vhs#readme"
}
